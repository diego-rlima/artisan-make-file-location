{
    "name": "diego-rlima/artisan-make-file-location",
    "description": "Ability to change the namespace/location of the files generated by the \"artisan make\" commands.",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Diego R. Lima",
            "email": "contact@diegoribeiro.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "DRL\\AMFL\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DRL\\AMFL\\ArtisanServiceProvider"
            ]
        }
    }
}
